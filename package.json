{
  "name": "sweepstake-lp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 2001",
    "build": "next build",
    "start": "next start",
    "lint:es": "eslint . --ext ts,tsx,js",
    "lint:style": "stylelint 'src/**/styled.ts' 'src/**/*.tsx'",
    "lint-staged": {
      "*.{ts,tsx,js,jsx}": [
        "eslint --cache"
      ],
      "styled.ts": [
        "stylelint --cache"
      ]
    },
    "lint:fix": "next lint --fix --ignore-path .eslintignore",
    "lint:css": "prettier '**/*.{json,yaml,css,scss}' --write --ignore-path .eslintignore",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "lint": "next lint",
    "husky": {
      "hooks": {
        "pre-commit": "yarn lint-staged && yarn check:type"
      }
    },
    "prepare": "husky install .husky",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@gsap/react": "^2.1.1",
    "@preact/signals-react": "^2.0.1",
    "@splinetool/react-spline": "^4.0.0",
    "@splinetool/runtime": "^1.7.4",
    "@studio-freight/react-lenis": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "classnames": "^2.3.2",
    "gsap": "^3.12.2",
    "next": "^14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-inlinesvg": "^4.1.0",
    "recharts": "^2.12.7",
    "sass": "^1.69.3",
    "split-type": "^0.3.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.0.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "stylelint": "^15.11.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5"
  }
}